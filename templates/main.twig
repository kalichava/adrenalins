{% block mainpageScripts %}
<script src="js/libs/jquery-ui.min.js"></script>
<script src="https://vjs.zencdn.net/4.5.1/video.js"></script>
<script src="js/libs/bigvideo.js"></script>
<script type='text/javascript'>
    {% if(mod != 'slideshow') %}
                    var BV = new $.BigVideo({container: $('.slide-container')});
                    BV.init();
                    if (window.innerWidth < 768) {
                        BV.show('img/slides/2.jpg');
                    } else {
                        BV.show('http://vjs.zencdn.net/v/oceans.mp4', {ambient: true, controls: true});
                        //BV.show('http://vimeo.com/9109887/download?t=1403739132&v=15037931&s=58d74c50be1a5b9c5205455b539b00ab', {ambient: true, controls: true});                        
                    }
    {% endif%}

                    var i = 1;
                    var mainpageSlides = function() {
                        switch (i++ % 3) {
                            case 0:
                                $('.slides-menu #club, .slide-club').removeClass('current').delay(1000).queue(function(n) {
                                    $('.slides-menu #school, .slide-school').addClass('current');
                                    n();
                                });
                                $('.slide-container.club').animate({opacity: 0}, 1000);
                                $('.slide-container.school').animate({opacity: 1}, 1000);
                                break;
                            case 1:
                                $('.slides-menu #school, .slide-school').removeClass('current').delay(1000).queue(function(n) {
                                    $('.slides-menu #tandem, .slide-tandem').addClass('current');
                                    n();
                                });
                                $('.slide-container.school').animate({opacity: 0}, 1000);
                                $('.slide-container.tandem').animate({opacity: 1}, 1000);
                                break;
                            case 2:
                                $('.slides-menu #tandem, .slide-tandem').removeClass('current').delay(1000).queue(function(n) {
                                    $('.slides-menu #club, .slide-club').addClass('current');
                                    n();
                                });
                                $('.slide-container.tandem').animate({opacity: 0}, 1000);
                                $('.slide-container.club').animate({opacity: 1}, 1000);
                                break;
                        }
                    };
                    var timer = setInterval(mainpageSlides, 7000);
                    $('h2, .action a').hover(function() {
                        clearInterval(timer);
                    }, function() {
                        timer = setInterval(mainpageSlides, 7000);
                    });
</script>
{% endblock mainpageScripts %}
