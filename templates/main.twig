{% extends "base.twig" %}
{% block pageTitle %}Main{% endblock %}
{% block slideContainer %}
    {% if(mod == 'slideshow') %}
<div class="slide-container school" style="background-image:url(img/slides/school.jpg);"></div>
<div class="slide-container tandem" style="background-image:url(img/slides/tandem.jpg);"></div>
<div class="slide-container club"   style="background-image:url(img/slides/club.jpg);"></div>
    {% else %}
<div class="slide-container"></div>
    {% endif %}
{% endblock  %}
{% block slides %}
<div class="mainpage slides">
